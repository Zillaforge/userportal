{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "trustworthy-platform",
  "description": "NCHC Trustworthy Platform",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "OCIS"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.12.5",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "serve:public": "vite --port 5173 --mode public --host",
    "serve:private": "vite --port 5173 --mode private --host",
    "serve:private-dev": "vite --port 5173 --mode private-dev --host",
    "build:clean": "rimraf dist",
    "build:public": "run-p eslint-check type-check build-public",
    "build:private": "run-p eslint-check type-check build-private",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview:public": "vite preview --mode public",
    "preview:private": "vite preview --mode private",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-public": "vite build --mode public",
    "build-private": "vite build --mode private",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "run-type-check": "run-p type-check",
    "prepare": "husky install",
    "eslint-check": "eslint --ext .ts,.vue src",
    "eslint-check-cache": "eslint --cache --ext .ts,.vue src",
    "cy:open": "cypress open --config-file cypress.config.mts",
    "cy:run-headed": "cypress run --config-file cypress.config.mts -b chrome --spec 'cypress/e2e/allTest.cy.ts' --headed",
    "cy:run": "cypress run --config-file cypress.config.mts -b chrome --spec 'cypress/e2e/allTest.cy.ts'",
    "cy:run-noExit": "cypress run --config-file cypress.config.mts -b chrome --spec 'cypress/e2e/allTest.cy.ts' --headed --no-exit",
    "cy:run-except-k8s": "cypress run --config-file cypress.config.mts -b chrome --spec 'cypress/e2e/allTestExceptK8s.cy.ts'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.665.0",
    "@aws-sdk/lib-storage": "3.665.0",
    "@aws-sdk/s3-request-presigner": "3.665.0",
    "@mdi/font": "^7.4.47",
    "@types/lodash": "^4.14.191",
    "@types/vue-chartkick": "^0.5.4",
    "axios": "1.7.9",
    "chart.js": "^4.4.4",
    "core-js": "^3.36.0",
    "dayjs": "1.11.6",
    "downloadjs": "^1.4.7",
    "http-status-codes": "^2.3.0",
    "jwt-decode": "^4.0.0",
    "lodash": "4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode.vue": "^3.5.1",
    "terser": "^5.29.1",
    "vue": "3.5.13",
    "vue-chartkick": "^1.1.0",
    "vue-cookies": "^1.8.4",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.3.0",
    "vuetify": "^3.6.8",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@tsconfig/node-lts": "^20.1.1",
    "@types/downloadjs": "^1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.25",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.16.0",
    "cypress-if": "^1.13.2",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "latest",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "eslint-plugin-vuetify": "^2.1.1",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "*",
    "vite": "5.3.6",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-static-copy": "2.2.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vuetify": "^2.0.2",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": ">=2.2.3",
    "nanoid": "3.3.8",
    "lodash": ">=4.17.21",
    "postcss": ">=8.4.49",
    "semver": ">=7.5.3",
    "vue": ">=3.5.13",
    "yaml": ">=2.3.2"
  },
  "stackblitz": {
    "startCommand": "yarn run test:unit"
  }
}
